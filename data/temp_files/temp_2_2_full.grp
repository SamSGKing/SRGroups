##This contains a list of the self-replicating groups on the rooted regular-2 tree on level 2##

BindGlobal("sr_2_2",
[
	[
		[ (1,3)(2,4), (1,2)(3,4) ],
		"SRGroup(2,2,1)",
		"SRGroup(2,1,1)",
		["the classes it extends to"]
	],

	[
		[ (1,3,2,4), (1,2)(3,4) ],
		"SRGroup(2,2,2)",
		"SRGroup(2,1,1)",
		["the classes it extends to"]
	],

	[
		[ (1,3)(2,4), (1,2) ],
		"SRGroup(2,2,3)",
		"SRGroup(2,1,1)",
		["the classes it extends to"]
	]